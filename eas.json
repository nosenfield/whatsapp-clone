{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "bundleIdentifier": "com.nosenfield.whatsappclone"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "simulator": false,
        "bundleIdentifier": "com.nosenfield.whatsappclone"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "1234567890",
        "appleTeamId": "TEAM_ID_HERE"
      }
    }
  }
}

